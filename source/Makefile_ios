OUTDIR = ../build_ios/
.PHONY: all FORCE clean
all: $(OUTDIR)LibTpBoost.a $(OUTDIR)LibJpegIjg6b.a $(OUTDIR)LibFgBase.a 
$(OUTDIR)LibTpBoost.a: $(OUTDIR)armv7/clang/release/LibTpBoost.a $(OUTDIR)arm64/clang/release/LibTpBoost.a $(OUTDIR)arm64e/clang/release/LibTpBoost.a $(OUTDIR)x64/clang/release/LibTpBoost.a
	xcrun -sdk iphoneos lipo -create -output $(OUTDIR)LibTpBoost.a $(OUTDIR)armv7/clang/release/LibTpBoost.a $(OUTDIR)arm64/clang/release/LibTpBoost.a $(OUTDIR)arm64e/clang/release/LibTpBoost.a $(OUTDIR)x64/clang/release/LibTpBoost.a
$(OUTDIR)LibJpegIjg6b.a: $(OUTDIR)armv7/clang/release/LibJpegIjg6b.a $(OUTDIR)arm64/clang/release/LibJpegIjg6b.a $(OUTDIR)arm64e/clang/release/LibJpegIjg6b.a $(OUTDIR)x64/clang/release/LibJpegIjg6b.a
	xcrun -sdk iphoneos lipo -create -output $(OUTDIR)LibJpegIjg6b.a $(OUTDIR)armv7/clang/release/LibJpegIjg6b.a $(OUTDIR)arm64/clang/release/LibJpegIjg6b.a $(OUTDIR)arm64e/clang/release/LibJpegIjg6b.a $(OUTDIR)x64/clang/release/LibJpegIjg6b.a
$(OUTDIR)LibFgBase.a: $(OUTDIR)armv7/clang/release/LibFgBase.a $(OUTDIR)arm64/clang/release/LibFgBase.a $(OUTDIR)arm64e/clang/release/LibFgBase.a $(OUTDIR)x64/clang/release/LibFgBase.a
	xcrun -sdk iphoneos lipo -create -output $(OUTDIR)LibFgBase.a $(OUTDIR)armv7/clang/release/LibFgBase.a $(OUTDIR)arm64/clang/release/LibFgBase.a $(OUTDIR)arm64e/clang/release/LibFgBase.a $(OUTDIR)x64/clang/release/LibFgBase.a
$(OUTDIR)armv7/clang/release/%.a: FORCE
	$(MAKE) -j4 -f Makefile_ios_armv7_clang_release
$(OUTDIR)arm64/clang/release/%.a: FORCE
	$(MAKE) -j4 -f Makefile_ios_arm64_clang_release
$(OUTDIR)arm64e/clang/release/%.a: FORCE
	$(MAKE) -j4 -f Makefile_ios_arm64e_clang_release
$(OUTDIR)x64/clang/release/%.a: FORCE
	$(MAKE) -j4 -f Makefile_ios_x64_clang_release
FORCE:
clean:
	rm -r $(OUTDIR)
